{
  "hosting": {
    "target": "sliptchatbot",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "predeploy": ["npm run build"]
  },
  "chatbot-hosting": {
    "target": "chatbot-hosting",
    "public": "bot",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}